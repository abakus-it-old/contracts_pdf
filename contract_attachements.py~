# -*- coding: utf-8 -*-
from openerp.osv import fields, orm

class Contract(models.Model)
	_inherit = 'account.analytic.account'

	# Add a new column with PDF
	

class AnalyticAccount(orm.Model):
    """ Add atatchements to Analytic Accounts """
    _inherit = 'account.analytic.account'

    _columns = {
		'attachements_ids': fields.One2many(
			'ir.attachment', string="Attached documents"),
        }

